{
  "name": "dog-friends",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "sh ./deploy.sh"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "buefy": "^0.7.5",
    "bulma": "^0.7.4",
    "register-service-worker": "^1.6.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.6",
    "vue-i18n": "^8.11.2",
    "vue-masonry": "^0.11.7",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-pwa": "^3.5.1",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "svg-sprite-loader": "^4.1.6",
